{
  "name": "My Bookmarks",
  "version": "1.1",
  "manifest_version": 2,
  "description": "description",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
    "default_title": "My Bookmarks",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}